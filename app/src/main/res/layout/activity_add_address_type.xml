<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:adjustViewBounds="true"
            android:scaleType="centerCrop"
            android:visibility="invisible"
            android:src="@drawable/bacjground" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:context=".activity.AddAddressTypeActivity">

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp"
                android:background="@color/colorLightGreen"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="@dimen/_3sdp">

                <ImageView
                    android:id="@+id/back"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:padding="@dimen/_7sdp"
                    android:src="@drawable/back"
                    android:tint="@color/white" />

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_23sdp"
                    android:layout_weight="1"
                    android:src="@drawable/logo_mangalhouse"
                    android:tint="@color/white" />

                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/back"
                    android:tint="@color/white"
                    android:visibility="invisible" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_30sdp"
                android:gravity="center"
                android:background="@drawable/round_green_border"
                android:orientation="horizontal">


                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1" />


                <ImageView
                    android:id="@+id/homeSelect"
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_margin="@dimen/_5sdp"
                    android:src="@drawable/radio_on"
                    android:tint="@color/colorLightGreen" />


                <com.angaihouse.controls.CTextView
                    android:id="@+id/tvHome"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/colorLightGreen"
                    android:textSize="@dimen/_12sdp" />


                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1" />


                <LinearLayout
                    android:id="@+id/llMale"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/maleUnSelect"
                        android:layout_width="@dimen/_15sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:layout_margin="@dimen/_5sdp"
                        android:src="@drawable/radio_blank"
                        android:tint="@color/colorLightGreen"
                        android:visibility="gone" />

                    <ImageView
                        android:id="@+id/workSelect"
                        android:layout_width="@dimen/_15sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:layout_margin="@dimen/_5sdp"
                        android:src="@drawable/radio_blank"
                        android:tint="@color/colorLightGreen" />

                    <com.angaihouse.controls.CTextView
                        android:id="@+id/tvWork"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:drawablePadding="@dimen/_8sdp"
                        android:gravity="center"
                        android:textColor="@color/colorLightGreen"
                        android:textSize="@dimen/_12sdp" />


                </LinearLayout>


                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1" />


                <LinearLayout
                    android:id="@+id/llFemale"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/otherSelect"
                        android:layout_width="@dimen/_15sdp"
                        android:layout_height="@dimen/_15sdp"
                        android:tint="@color/colorLightGreen"
                        android:src="@drawable/radio_blank"
                        android:layout_margin="@dimen/_5sdp" />

                    <com.angaihouse.controls.CTextView
                        android:id="@+id/tvOther"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:drawablePadding="@dimen/_8sdp"
                        android:gravity="center"
                        android:textColor="@color/colorLightGreen"
                        android:textSize="@dimen/_12sdp" />


                </LinearLayout>


                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_30sdp"
                android:background="@drawable/round_green_border"
                android:gravity="center_vertical"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:src="@drawable/pin"
                    android:tint="@color/colorLightGreen" />


                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/colorLightGreen" />


                <com.seatgeek.placesautocomplete.PlacesAutocompleteTextView
                    android:id="@+id/atv_places"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:pacv_googleMapsApiKey="AIzaSyDnwVsJT81wVvk3ER1kbjebezFzBTzu4i4"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:inputType="textPersonName"
                    android:padding="@dimen/_5sdp"
                    android:background="@null"
                    android:textSize="@dimen/_12sdp"
                    android:textColor="@color/colorLightGreen"
                    app:is_required="true"/>

                <!-- <AutoCompleteTextView
                     android:id="@+id/atv_places"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="center"
                     android:ellipsize="end"
                     android:gravity="center_vertical"
                     android:maxLines="2"
                     android:hint="Address"
                     android:padding="@dimen/_5sdp"
                     android:background="@null"
                     android:textSize="@dimen/_12sdp"
                     android:textColor="@color/colorLightGreen"
                     app:is_required="true" />-->

                <!--<com.angaihouse.controls.CEditText
                    android:id="@+id/email"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/corner_border"
                    android:hint="Address*"
                    android:inputType="textPersonName"
                    android:padding="@dimen/_7sdp"
                    android:layout_margin="0.01dp"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_11sdp" />-->


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_30sdp"
                android:background="@drawable/round_green_border"
                android:gravity="center_vertical"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:src="@drawable/pin"
                    android:tint="@color/colorLightGreen" />

                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/colorLightGreen" />


                <com.angaihouse.controls.CEditText
                    android:id="@+id/address2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:inputType="textPersonName"
                    android:padding="@dimen/_5sdp"
                    android:background="@null"
                    android:textSize="@dimen/_12sdp"
                    android:textColor="@color/colorLightGreen" />

                <!-- <AutoCompleteTextView
                     android:id="@+id/atv_places"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="center"
                     android:ellipsize="end"
                     android:gravity="center_vertical"
                     android:maxLines="2"
                     android:hint="Address"
                     android:padding="@dimen/_5sdp"
                     android:background="@null"
                     android:textSize="@dimen/_12sdp"
                     android:textColor="@color/colorLightGreen"
                     app:is_required="true" />-->

                <!--<com.angaihouse.controls.CEditText
                    android:id="@+id/email"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/corner_border"
                    android:hint="Address*"
                    android:inputType="textPersonName"
                    android:padding="@dimen/_7sdp"
                    android:layout_margin="0.01dp"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_11sdp" />-->


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_30sdp"
                android:background="@drawable/round_green_border"
                android:gravity="center_vertical"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:src="@drawable/company"
                    android:tint="@color/colorLightGreen" />


                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/colorLightGreen" />


                <com.angaihouse.controls.CEditText
                    android:id="@+id/doorNumber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:padding="@dimen/_5sdp"
                    android:background="@null"
                    android:textColor="@color/colorLightGreen"
                    android:textSize="@dimen/_12sdp" />


            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_30sdp"
                android:background="@drawable/round_green_border"
                android:gravity="center_vertical"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:src="@drawable/city"
                    android:tint="@color/colorLightGreen" />


                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/colorLightGreen" />


                <com.angaihouse.controls.CEditText
                    android:id="@+id/city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:padding="@dimen/_5sdp"
                    android:background="@null"
                    android:textColor="@color/colorLightGreen"
                    android:textSize="@dimen/_12sdp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_30sdp"
                android:background="@drawable/round_green_border"
                android:gravity="center_vertical"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:src="@drawable/postal_code"
                    android:tint="@color/colorLightGreen" />


                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/colorLightGreen" />


                <com.angaihouse.controls.CEditText
                    android:id="@+id/ZipCode"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:padding="@dimen/_5sdp"
                    android:background="@null"
                    android:textColor="@color/colorLightGreen"
                    android:textSize="@dimen/_12sdp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_30sdp"
                android:background="@drawable/round_green_border"
                android:gravity="center_vertical"
                android:orientation="horizontal">


                <ImageView
                    android:layout_width="@dimen/_15sdp"
                    android:layout_height="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:src="@drawable/country"
                    android:tint="@color/colorLightGreen" />


                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/colorLightGreen" />


                <com.angaihouse.controls.CEditText
                    android:id="@+id/country"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:background="@null"
                    android:padding="@dimen/_5sdp"
                    android:textColor="@color/colorLightGreen"
                    android:textSize="@dimen/_12sdp" />


            </LinearLayout>


            <View
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <com.angaihouse.controls.CTextView
                android:id="@+id/saveAddress"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@drawable/round_green_solid"
                android:layout_margin="@dimen/_20sdp"
                android:gravity="center"
                android:padding="@dimen/_7sdp"
                android:textColor="@color/white"
                android:textSize="@dimen/_13sdp" />

        </LinearLayout>

    </FrameLayout>

</layout>